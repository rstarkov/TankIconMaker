  <Layer>
    <Visible>
      <DisplayName>
        <Translation>Sichtbar</Translation>
        <Old>Visible</Old>
      </DisplayName>
      <Description>
        <Translation>Hiermit können Sie die Ebene unsichtbar machen, ohne sie zu löschen.</Translation>
        <Old>Allows you to hide this layer without deleting it.</Old>
      </Description>
    </Visible>
    <VisibleFor>
      <DisplayName>
        <Translation>Visible for</Translation>
        <Old null="1" />
      </DisplayName>
      <Description>
        <Translation>Specifies which types of tanks this layer should be visible for.</Translation>
        <Old null="1" />
      </Description>
    </VisibleFor>
  </Layer>
  <BkgDarkAgentLayer>
    <LayerName>
      <Translation>Hintergrund / Dark Agent</Translation>
      <Old>Background / Dark Agent</Old>
    </LayerName>
    <LayerDescription>
      <Translation>Zeichnet einen Hintergrund mit einem glasähnlichen Design, inspiriert durch die Symbolsammlung von Black_Spy.</Translation>
      <Old>Draws a background using a glassy style inspired by Black_Spy’s icon set.</Old>
    </LayerDescription>
    <BackColor>
      <DisplayName>
        <Translation>Hintergrundfarbe</Translation>
        <Old>Background color</Old>
      </DisplayName>
      <Description>
        <Translation>Hintergrundfarbe.</Translation>
        <Old>Background color.</Old>
      </Description>
    </BackColor>
  </BkgDarkAgentLayer>
  <TankImageLayer>
    <LayerName>
      <Translation>Bild / Standard</Translation>
      <Old>Image / Standard</Old>
    </LayerName>
    <LayerDescription>
      <Translation>Zeichnet eine der vielen Arten von Standardbildern für diesen Panzer.</Translation>
      <Old>Draws one of the several types of standard images for this tank.</Old>
    </LayerDescription>
    <Style>
      <DisplayName>
        <Translation>Typ</Translation>
        <Old>Type</Old>
      </DisplayName>
      <Description>
        <Translation>Bestimmt, welcher Standardbildtyp gezeichnet wird.</Translation>
        <Old>Specifies which of the standard image types to draw.</Old>
      </Description>
    </Style>
    <MissingImageWarning>
      <Translation>Das Bild für diesen Panzer fehlt.</Translation>
      <Old>The image for this tank is missing.</Old>
    </MissingImageWarning>
  </TankImageLayer>
  <CurrentImageLayer>
    <LayerName>
      <Translation>Bild / Aktuell</Translation>
      <Old>Image / Current</Old>
    </LayerName>
    <LayerDescription>
      <Translation>Zeichnet das Symbol, das aktuell im Ausgabeverzeichnis gespeichert ist.</Translation>
      <Old>Draws the icon that’s currently saved in the output directory.</Old>
    </LayerDescription>
    <MissingImageWarning>
      <Translation>Für diesen Panzer liegt kein aktuelles Bild vor.</Translation>
      <Old>There is no current image for this tank.</Old>
    </MissingImageWarning>
  </CurrentImageLayer>
  <CustomImageLayer>
    <LayerName>
      <Translation>Bild / Nach Eigenschaften</Translation>
      <Old>Image / By properties</Old>
    </LayerName>
    <LayerDescription>
      <Translation>Zeichnet ein beliebiges benutzerdefiniertes Bild.</Translation>
      <Old>Draws an arbitrary, user-supplied image.</Old>
    </LayerDescription>
    <MissingImageWarning>
      <Translation>Das Bild {0} konnte nicht gefunden werden.</Translation>
      <Old>The image {0} could not be found.</Old>
    </MissingImageWarning>
  </CustomImageLayer>
  <FilenamePatternImageLayer>
    <LayerName>
      <Translation>Bild / Nach Dateinamenschablone</Translation>
      <Old>Image / By filename pattern</Old>
    </LayerName>
    <LayerDescription>
      <Translation>Zeichnet ein benutzerdefiniertes Bild, das aus einer Datei eingelesen wird, deren Name einer Schablone entspricht, in der Platzhalter durch verschiedene Panzereigenschaften eingesetzt werden.</Translation>
      <Old>Draws an arbitrary, user-supplied image, loaded from a file whose name is generated by substituting various tank properties into placeholders.</Old>
    </LayerDescription>
    <Pattern>
      <DisplayName>
        <Translation>Schablone</Translation>
        <Old>Pattern</Old>
      </DisplayName>
      <Description>
        <Translation>Dateinamenschablone. Die folgenden Platzhalter sind verfügbar: {tier} (Panzerstufe), {country} (Land), {class} (Klasse), {category} (Kategorie), {id} (ID).</Translation>
        <Old>Filename pattern. Use the following placeholders: {tier}, {country}, {class}, {category}, {id}.</Old>
      </Description>
    </Pattern>
    <MissingImageWarning>
      <Translation>Das Bild {0} konnte nicht gefunden werden.</Translation>
      <Old>The image {0} could not be found.</Old>
    </MissingImageWarning>
  </FilenamePatternImageLayer>
